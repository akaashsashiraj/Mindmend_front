{
  "expo": {
    "name": "front",
    "slug": "front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/finalLogo.png",
    "scheme": "front",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/finalLogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#042558"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/finalLogo.png",
        "backgroundColor": "#042558"
      },
      "splash": {
        "image": "./assets/images/finalLogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#042558"
      },
      "package": "com.anonymous.front"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/finalLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/finalLogo.png",
          "image2": "./assets/images/hand.png",
          "resizeMode": "contain",
          "backgroundColor": "#042558"
        }
      ],
      [
        "expo-font",
        {
          "font": [
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Poppins-Bold.ttf",
            "./assets/fonts/Poppins-BoldItalic.ttf",
            "./assets/fonts/Poppins-ExtraBold.ttf",
            "./assets/fonts/Poppins-ExtraBoldItalic.ttf",
            "./assets/fonts/Poppins-ExtraLight.ttf",
            "./assets/fonts/Poppins-ExtraLightItalic.ttf",
            "./assets/fonts/Poppins-Italic.ttf",
            "./assets/fonts/Poppins-Light.ttf",
            "./assets/fonts/Poppins-LightItalic.ttf",
            "./assets/fonts/Poppins-Medium.ttf",
            "./assets/fonts/Poppins-MediumItalic.ttf",
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf",
            "./assets/fonts/Poppins-SemiBoldItalic.ttf",
            "./assets/fonts/Poppins-Thin.ttf",
            "./assets/fonts/Poppins-ThinItalic.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
